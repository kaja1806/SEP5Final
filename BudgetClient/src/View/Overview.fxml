<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.OverviewController">
    <children>
        <Button fx:id="statusBtn" layoutX="414.0" layoutY="174.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="101.0" text="Show Status" />
        <PieChart fx:id="pieChart" layoutX="40.0" layoutY="79.0" prefHeight="307.0" prefWidth="287.0" />
        <MenuBar layoutX="370.0" layoutY="29.0" prefHeight="25.0" prefWidth="189.0">
            <menus>
                <Menu mnemonicParsing="false" text="Settings">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Account" />
                        <MenuItem mnemonicParsing="false" text="Card" onAction="#addCard" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Add">
                    <items>
                        <MenuItem mnemonicParsing="false" text="New user" />
                        <MenuItem mnemonicParsing="false" text="New card" />
                        <MenuItem mnemonicParsing="false" onAction="#makePayment" text="New expense" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Premium">
                    <items>
                        <MenuItem mnemonicParsing="false" text="About" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
    </children>
</AnchorPane>
